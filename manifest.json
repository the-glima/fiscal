{
  "name" : "fiscal",
  "version" : "0.1.0",
  "description" : "A Chrome Extension for checking PRs matches.",
  "browser_action": {
     "default_icon": "src/images/icon.png",
     "default_popup": "src/html/popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://dev.azure.com/payvision/B-Ops/_git/*"],
      "js" : ["dist/popup.js"]
    }
  ]
}
